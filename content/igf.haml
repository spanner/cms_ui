    %link{rel: "icon", type: "image/png", href: "/images/favicon-32.png", size: '32x32'}
    %link{rel: "apple-touch-icon-precomposed", href: "/images/favicon-152.png"}
    /[if IE]
      %link{rel: "shortcut icon", href: "/images/favicon.ico"}
  
  %body
    #masthead
      %header
        #nav_menu_head
          %a.menu{href: "#nav_menu"}
        #logo
          %a.full_logo{href: "/"}
        #nav
          .section
            %a.head{href: "/"} Home
          .section
            %a.head{href: "/foundation"} Foundation
            %ul.detail
              %li
                %a{href: "/foundation/story"} Invictus story
              %li
                %a{href: "/foundation/governance"} Governance
          .section
            %a.head{href: "/games"} Games
            %a.head{href: "/news"} News
            %ul.detail
              %li
                %a{href: "/games/2014"} London 2014
              %li
                %a{href: "/games/2016"} Orlando 2016
          .section
            %a.head{href: "/contact"} Contact
            %ul.detail
              %a{href: "/contact/faq"} FAQ

        #links.links
          %a.twitter{href: "https://twitter.com/weareinvictus", target: "_blank"}
          %a.facebook{href: "https://www.facebook.com/invictuslondon", target: "_blank"}
          %a.youtube{href: "https://www.youtube.com/user/invictuslondon", target: "_blank"}
          .detail
            %a.twitterbird{href: "https://twitter.com/weareinvictus", target: "_blank"} weareinvictus

    #body
      %main

